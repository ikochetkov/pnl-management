        <name>getAskDeadlineByPeriod</name>
        <operation_script><![CDATA[(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

    var month = request.queryParams.month[0];
	var year = request.queryParams.year[0];
	
	var deadline = new Utils().getDeadline(year, month, 'question');
	var timeValid = new Utils().timeValid(deadline.start, deadline.end);
	return {
		time_valid: timeValid,
		start: deadline.start,
		end: deadline.end
	};

})(request, response);]]></operation_script>
