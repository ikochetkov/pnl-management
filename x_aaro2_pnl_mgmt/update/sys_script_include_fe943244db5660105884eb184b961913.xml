        <name>CC_Utils</name>
        <script><![CDATA[var CC_Utils = Class.create();
CC_Utils.prototype = {
    initialize: function() {
    },
	
	getAssignments: function(acc){
		var users = [];
		var accountAssignmentGr = new GlideRecord('x_aaro2_pnl_mgmt_pnl_account_assignment');
		accountAssignmentGr.addQuery('account', acc);
		accountAssignmentGr.query();
		while(accountAssignmentGr.next()){
			users.push(accountAssignmentGr.getValue('user'));
		}
		return users;
	},

    type: 'CC_Utils'
};]]></script>
