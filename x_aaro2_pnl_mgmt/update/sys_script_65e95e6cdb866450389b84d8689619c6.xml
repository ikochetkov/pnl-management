        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	var participantsTable = 'x_aaro2_pnl_mgmt_m2m_statements_verificators'
	var dlGr = new GlideRecord('x_aaro2_pnl_mgmt_deadline')
	dlGr.orderByDesc('sys_created_on');
	dlGr.setLimit(1);
	dlGr.query();
	dlGr.next();
	var prevDl = dlGr.getUniqueValue();
	
	var prevDlParticipant = new GlideRecord(participantsTable)
	prevDlParticipant.addQuery('deadline', prevDl);
	prevDlParticipant.query()
	while(prevDlParticipant.next()){
		var newParticipant = new GlideRecord(participantsTable);
		newParticipant.user = prevDlParticipant.user;
		newParticipant.deadline = current.getUniqueValue();
		newParticipant.insert();
	}

})(current, previous);]]></script>
